  pod  'FBSDKLoginKit' 
  pod 'TwitterKit' 
  pod 'Alamofire'
  pod 'SwiftyJSON'

  target 'WebviewTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WebviewUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
